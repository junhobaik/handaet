{
  "manifest_version": 2,
  "name": "Only Hangul comments in Youtube",
  "description": "",
  "version": "0.1",

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": ["*://*.youtube.com/*", "storage"],

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["watch.js"]
    }
  ]
}
